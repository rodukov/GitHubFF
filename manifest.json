{
    "name": "GitHub FF",
    "description": "An extension for customizing your GitHub profile page",
    "version": "v1.0.0",
    "manifest_version": 2,
    "browser_action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["script.js"],
            "css": ["style.css"]
        }
    ],
    "permissions": ["storage"]
}